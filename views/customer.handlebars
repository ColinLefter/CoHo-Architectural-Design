<h1>Customer Profile</h1>
{{#if customer}}
    <table border='1'>
        <tr><td><strong>Id</strong></td><td>{{customer.customerId}}</td></tr>
        <tr><td><strong>First Name</strong></td><td>{{customer.firstName}}</td></tr>
        <tr><td><strong>Last Name</strong></td><td>{{customer.lastName}}</td></tr>
        <tr><td><strong>Email</strong></td><td>{{customer.email}}</td></tr>
        <tr><td><strong>Phone</strong></td><td>{{customer.phonenum}}</td></tr>
        <tr><td><strong>Address</strong></td><td>{{customer.address}}</td></tr>
        <tr><td><strong>City</strong></td><td>{{customer.city}}</td></tr>
        <tr><td><strong>State</strong></td><td>{{customer.state}}</td></tr>
        <tr><td><strong>Postal Code</strong></td><td>{{customer.postalCode}}</td></tr>
        <tr><td><strong>Country</strong></td><td>{{customer.country}}</td></tr>
        <tr><td><strong>User Id</strong></td><td>{{customer.userid}}</td></tr>
    </table>
{{else}}
    <p>{{message}}</p>
{{/if}}